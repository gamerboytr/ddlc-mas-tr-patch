init 5 python:
    addEvent(
        Event(
            persistent.event_database,
            eventlabel="monika_detected_console_cheater",
            conditional=(
                "config.console == True and "
                "not seen_event('monika_detected_console_cheater')"
            )
        )
    )

label monika_detected_console_cheater:
    m 4gub "Hey, [player]."
    m 2tub "Grdm kadaryla hile yapyorsun!"
    m 2tua "Bu dorumudur?"
    menu:
        "Evet.":
            m 1eub "Yalan sylemediin iin teekkrler!"
            m 7hub "Beni asla yanltmayacan biliyordum!"
            m 3eub "Ve konsolu atn iin sana kzmadm.{nw}"
            extend 1gub "eminimki nemli birey iin amsndr."
        "Hayr.":
            m 7tub "Yalan sylyorsun, [player]!"
            m 1tub "Grdm kadaryla konsolu amsn."
            m 7gub "Hatta yle kontrol edelim."
            show monika 7guu
            call updateconsole("config.console == True", "True")
            m 7tub "Grdn m, [player]?"
            call hideconsole()
            m 1eub "Bir daha bana yalan syleme, olur mu?"
    if mas_isMoniNormal(higher=True):
        m 7hub "Seni ok seviyorum bunu asla unutma bunu olur mu?"
        show monika 1eua
        return "love"
    else:
        m 2guo "Bir daha yapma bunu."
        show monika 1eua
        return "no_unlock"